{
  "compilerOptions": {
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es5",
    "jsx": "react",
    "noEmitOnError": true,
    "sourceMap": false,
    "declaration": true,
    "strict": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "lib": ["esnext"],
    "types": ["@pixinsight/core/types/globals"]
  }
}
